{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set gridName = 'demac-wufoo-credentials' %}
{% set pageTitle = 'Wufoo Credentials' %}


{% block navButtons %}
    {% if resource_granted('demac_wufoo_credentials_view_acl') %}
        <div class="btn-group">
            {{ UI.addButton({
                'path': path('demac_wufoo_credentials_create'),
                'entity_label': 'Add new Wufoo Form'
            }) }}
        </div>
    {% endif %}
{% endblock %}


{% block content_data %}
    {{ UI.renderGrid(gridName) }}
{% endblock content_data %}